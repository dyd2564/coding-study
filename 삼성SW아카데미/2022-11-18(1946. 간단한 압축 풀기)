T = int(input())

for tc in range(1, T+1):
    a = int(input())
    val = ""
    for i in range(a):
        string, num = input().split()
        num = int(num)
        val += string*num

    print('#{}'.format(tc))
    for i in range(len(val)):
        if (i+1)%10 == 0:
            print(val[i])
        else:
            print(val[i], end='')
    print()
